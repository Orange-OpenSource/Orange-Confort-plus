	"visionPlusName": {
		"message": "Vision+"
	},
	"visionPlusDescription": {
		"message": "Mode of use designed for people with visual limitations (colour, legibility). The colours of the text and page are high-contrast, the mouse and focus are clearly visible, and a magnifying glass and reading ruler are available."
	},
	"visionPlusPlusName": {
		"message": "Vision++"
	},
	"visionPlusPlusDescription": {
		"message": "Mode of use for people with severe visual impairments. The text and page colors are modified, elements (focus, mouse, text, scrollbar) are much larger, an optimized font, a ×5 magnifier, and a reading ruler are available."
	},
	"facilePlusName": {
		"message": "Easy+"
	},
	"facilePlusDescription": {
		"message": "Mode of use for people not very familiar with the internet. The text, buttons, links, mouse, and focus are very visible and slightly larger, contrasts are adjusted, and a magnifier is available."
	},
	"lecturePlusName": {
		"message": "Reading+"
	},
	"lecturePlusDescription": {
		"message": "A mode of use for people with reading or writing difficulties. A font optimised for dyslexia, airy, left-aligned text, a reading ruler, the option of colouring the sound characters and having the text read to you."
	},
	"lecturePlusPlusName": {
		"message": "Reading++"
	},
	"lecturePlusPlusDescription": {
		"message": "Mode of use intended for people with reading and/or handling constraints. A font optimized for dyslexia, clearly readable texts, reading rules, the possibility of coloring characters-sounds and to be read the text, as well as aids to pointing."
	},
	"moteurPlusName": {
		"message": "Motor+"
	},
	"moteurPlusDescription": {
		"message": "Mode of use for people with motor disabilities. Easier keyboard navigation, a highly visible focus, and a palette of virtual buttons."
	},
	"moteurPlusPlusName": {
		"message": "Motor++"
	},
	"moteurPlusPlusDescription": {
		"message": "Mode of use designed for people using a single switch. Automatic scrolling of the focus in the web page, with adjustable speed and delay."
	},
	"pointagePlusName": {
		"message": "Pointing+"
	},
	"pointagePlusDescription": {
		"message": "Mode of use for people with mouse pointing difficulties. A highly visible and large mouse, enlarged click areas, and the possibility to click by hovering without effort, with large button scrollbars."
	},
	"langueDesSignesName": {
		"message": "Sign Language"
	},
	"langueDesSignesDescription": {
		"message": "Mode of use for people who primarily communicate in sign language. It provides sign language thumbnails on complex words, a link to the Signs@Work dictionary, and an interface with videosigns."
	},
	"audioPlusName": {
		"message": "Audio+"
	},
	"audioPlusDescription": {
		"message": "Mode of use for people with hearing difficulties. Visual equivalents to audio signals, systematic subtitling."
	},
	"calmePlusName": {
		"message": "Serene+"
	},
	"calmePlusDescription": {
		"message": "Mode of use that meets the constraints of hypersensoriality. Pleasant colours and transitions, no animations or interruptions."
	},
