	"visionPlusName": {
		"message": "Vision+"
	},
	"visionPlusDescription": {
		"message": "User mode designed for people with visual limitations (colour, legibility). The colours of the text and page are high-contrast, the mouse and focus are clearly visible, and a magnifying glass and reading ruler are available."
	},
	"visionPlusPlusName": {
		"message": "Vision++"
	},
	"visionPlusPlusDescription": {
		"message": "Usage mode for people with severe visual impairments. The text and page colors are modified, elements (focus, mouse, text, scrollbar) are much larger, an optimized font, a ×5 magnifier, and a reading ruler are available."
	},
	"facilePlusName": {
		"message": "Easy+"
	},
	"facilePlusDescription": {
		"message": "Usage mode for people not very familiar with the internet. The text, buttons, links, mouse, and focus are very visible and slightly larger, contrasts are adjusted, and a magnifier is available."
	},
	"dysPlusName": {
		"message": "Dys+"
	},
	"dysPlusDescription": {
		"message": "A user guide for people with reading or writing difficulties. A font optimised for dyslexia, airy, left-aligned text, a reading ruler, the option of colouring the sound characters and having the text read to you."
	},
	"dysPlusPlusName": {
		"message": "Dys++"
	},
	"dysPlusPlusDescription": {
		"message": "This mode has not yet been described"
	},
	"moteurPlusName": {
		"message": "Motor+"
	},
	"moteurPlusDescription": {
		"message": "Usage mode for people with motor disabilities. Easier keyboard navigation, a highly visible focus, and a palette of virtual buttons."
	},
	"moteurPlusPlusName": {
		"message": "Motor++"
	},
	"moteurPlusPlusDescription": {
		"message": "Use mode designed for people using a single switch. Automatic scrolling of the focus in the web page, with adjustable speed and delay."
	},
	"pointagePlusName": {
		"message": "Pointing+"
	},
	"pointagePlusDescription": {
		"message": "Usage mode for people with mouse pointing difficulties. A highly visible and large mouse, enlarged click areas, and the possibility to click by hovering without effort, with large button scrollbars."
	},
	"langueDesSignesName": {
		"message": "Sign Language"
	},
	"langueDesSignesDescription": {
		"message": "Usage mode for people who primarily communicate in sign language. LSF thumbnails on complex words, a link to the Signs@Work dictionary, and a video_sign interface."
	},
	"audioPlusName": {
		"message": "Audio+"
	},
	"audioPlusDescription": {
		"message": "User guide for people with hearing difficulties. Visual equivalents to audio signals, systematic subtitling."
	},
	"calmePlusName": {
		"message": "Calm+"
	},
	"calmePlusDescription": {
		"message": "Mode of use that meets the constraints of hypersensoriality. Pleasant colours and transitions, no animations or interruptions."
	},
